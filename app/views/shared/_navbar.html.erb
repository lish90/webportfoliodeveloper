<div class="navbar navbar-expand-sm navbar-light navbar-lewagon">
  <%= link_to root_path, class: "navbar-bar" do%>
    <%= image_tag "iconmonstr-code-8.svg"%> 
  <% end %>
  
  <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
    <span class="navbar-toggler-icon"></span>
  </button>
    


  <div class="collapse navbar-collapse" id="navbarSupportedContent">
    <ul class="navbar-nav mr-auto">
      <li class="nav-item active">
        <%= link_to "Home", "root_path", class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "About", "about_path", class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Skills", "skills_path", class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Project", "project_path", class: "nav-link" %>
      </li>
      <li class="nav-item">
        <%= link_to "Contact", "contact_path", class: "nav-link"%>
      </li>
      <li class= "nav-item dropdown">
       <%=image_tag "iconmonstr-code-fork-2.svg", class: "avatar dropdown-toggle", id: "navbarDropdown", data: { toggle: "dropdown" }, 'aria-haspopup': true, 'aria-expanded': false %>
        <div class="dropdown-menu dropdown-menu-right nav-item dropdown" aria-labelledby="navbarDropdown">
          <%= link_to "Home", "root_path", class: "dropdown-item" %>
          <%= link_to "About", "about_path", class: "dropdown-item" %>
          <%= link_to "Skills", "skills_path", class: "dropdown-item" %>
          <%= link_to "Projects", "project_path", class: "dropdown-item" %>
          <%= link_to "Contact", "contact_path", class: "dropdown-item" %>
        </div>
      </li>
    </ul>
  </div>
</div>
